{
  "name": "deep-architecture",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "dev": "NEXT_DIST_DIR=.next-dev next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "docs:dev": "vitepress dev docs --port 3001",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 3001",
    "docs:guard": "node scripts/check-doc-updates.mjs",
    "docs:guard:staged": "node scripts/check-doc-updates.mjs --staged",
    "build:worker": "tsc -p tsconfig.worker.json",
    "worker": "pnpm build:worker && node .tsbuild/worker.js",
    "worker:env": "dotenv -e .env.local -- sh -c \"pnpm build:worker && node .tsbuild/worker.js\""
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "next": "14.2.35",
    "openai": "5.12.0",
    "pg": "8.11.5",
    "pg-boss": "12.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reactflow": "11.11.4"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "@types/pg": "8.11.6",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.35",
    "typescript": "5.4.5",
    "vitepress": "^1.6.4",
    "vitest": "2.1.9"
  }
}
